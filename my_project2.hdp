lib1 = $HDS_PROJECT_DIR/lib1/hdl
lib2 = $HDS_PROJECT_DIR/lib2/hdl
lib3 = $HDS_PROJECT_DIR/lib3/hdl
UART = $HDS_PROJECT_DIR/UART/hdl
UART_MultiLib = $HDS_PROJECT_DIR/UART_MultiLib/hdl
[hds]
