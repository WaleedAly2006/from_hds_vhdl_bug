lib1 = $HDS_PROJECT_DIR/lib1/hds
lib2 = $HDS_PROJECT_DIR/lib2/hds
lib3 = $HDS_PROJECT_DIR/lib3/hds
UART = $HDS_PROJECT_DIR/UART/hds
UART_MultiLib = $HDS_PROJECT_DIR/UART_MultiLib/hds
[hds_settings]
